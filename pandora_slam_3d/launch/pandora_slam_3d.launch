<launch>
  
  <node pkg="image_view" type="image_view" name="image_view" respawn="false" output="log">
		<remap from="/image" to="/ORB_SLAM/Frame" />
		<param name="autosize" value="true"/>
	</node>
	<node pkg="rviz" type="rviz" name="rviz" args="-d $(find pandora_slam_3d)/param/rviz.rviz" output="log"/>

	<!-- Start slam node -->
  <node name="pandora_slam_3d_node" type="pandora_slam_3d_node" pkg="pandora_slam_3d" ns="pandora_slam"/>
  <rosparam file="$(find pandora_slam_3d)/param/orb_slam_parameters.yaml" command="load" ns="orb_slam"/>

  
</launch>
